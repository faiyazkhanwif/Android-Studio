<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/Fdemo"
        android:layout_width="225dp"
        android:layout_height="56dp"
        android:fontFamily="casual"
        android:text="@string/fdemo"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.168" />

    <Button
        android:id="@+id/btnpeng"
        android:layout_width="381dp"
        android:layout_height="60dp"
        android:fontFamily="casual"
        android:text="@string/penguin"
        app:backgroundTint="#808183"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Fdemo"
        app:layout_constraintVertical_bias="0.066" />

    <Button
        android:id="@+id/btnrab"
        android:layout_width="381dp"
        android:layout_height="60dp"
        android:fontFamily="casual"
        android:text="@string/rabbit"
        app:backgroundTint="#808183"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnpeng"
        app:layout_constraintVertical_bias="0.0" />

    <FrameLayout
        android:id="@+id/fragoutput"
        android:layout_width="243dp"
        android:layout_height="288dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnrab"
        app:layout_constraintVertical_bias="0.116" />
</androidx.constraintlayout.widget.ConstraintLayout>